<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="460.0" prefWidth="206.0" spacing="10.0">
         <children>
            <Label text="CheckBox Objects">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Label>
            <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
            <CheckBox fx:id="pinneappleCheckBox" mnemonicParsing="false" text="Pineapple" />
            <CheckBox fx:id="baconCheckBox" mnemonicParsing="false" text="Bacon" />
            <Button mnemonicParsing="false" onAction="#pizzaOrdderButtonOrderPushed" text="Order Pizza" />
            <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" prefHeight="94.0" prefWidth="196.0" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
